{
  "name": "grpc-ts",
  "version": "0.0.0",
  "main": "index.js",
  "license": "MIT",
  "files": [
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "build": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./ --grpc_out=grpc_js:./ -I ../proto ../proto/*.proto && grpc_tools_node_protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=grpc_js:. -I ../proto ../proto/*.proto",
    "dev": "nodemon --watch ../proto --exec \"yarn build\" --delay 1 --ext proto"
  },
  "devDependencies": {
    "grpc-proto": "*",
    "grpc-tools": "^1.12.3",
    "grpc_tools_node_protoc_ts": "^5.3.2",
    "nodemon": "^2.0.20"
  },
  "publishConfig": {
    "access": "public"
  }
}
